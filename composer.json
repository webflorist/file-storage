{
    "name": "webflorist/file-storage",
    "description": "DB-based file management for Laravel 5.8+",
    "authors": [
        {
            "name": "Gerald Buttinger",
            "email": "gerald@code.florist"
        }
    ],
    "require": {
        "php": "~7.1",
		"laravel/framework": "~5.8",
        "jbroadway/urlify": "^1.1",
        "intervention/image": "^2.5",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8"
    },
    "autoload": {
        "psr-4": {
            "Webflorist\\FileStorage\\": "src/",
            "FileStorageTests\\": "tests/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webflorist\\FileStorage\\FileStorageServiceProvider"
            ],
            "aliases": {
                "FileStorage": "Webflorist\\FileStorage\\FileStorageFacade"
            }
        }
    }
}
